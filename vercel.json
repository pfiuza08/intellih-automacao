{
  "version": 2,
  "headers": [
    {
      "source": "/ebooks/(.*).pdf",
      "headers": [
        { "key": "Content-Type", "value": "application/pdf" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "formulario.intellih.com.br" }],
      "destination": "/formulario.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "thankyou.intellih.com.br" }],
      "destination": "/thankyou.html"
    },
    {
      "source": "/thankyou",
      "destination": "/thankyou.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

